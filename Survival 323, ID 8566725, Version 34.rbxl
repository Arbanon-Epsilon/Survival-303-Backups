						<string name="Source">local tool = script.Parent
local on = tool.Name
local handle = tool.Handle

local headDamage = 2
local torsoDamage = 1
local armsDamage = 0.5
local legsDamage = 0.75
local damage = 15
local reloadTime = 1
local range = 10
local debounce = true

function slash()
	local s = Instance.new(&quot;StringValue&quot;)
	s.Name = &quot;toolanim&quot;
	s.Value = &quot;Slash&quot;
	s.Parent = tool
end

function onButton1Down(mouse)
	if debounce == true then

		slash()

		local target = mouse.Target
		local pos = mouse.hit.p

		if target ~= nil then

			if target.Parent ~= nil then

				if (handle.Position - target.Position).magnitude &lt; range then

					local h = target.Parent:findFirstChild(&quot;Humanoid&quot;)

					if h ~= nil then

						if target.Name == &quot;Head&quot; then
							h:TakeDamage(headDamage * damage)
						end

						if target.Name == &quot;Torso&quot; then
							h:TakeDamage(torsoDamage * damage)
						end

						if target.Name == &quot;Left Arm&quot; or target.Name == &quot;Right Arm&quot; then
							h:TakeDamage(armsDamage * damage)
						end

						if target.Name == &quot;Left Leg&quot; or target.Name == &quot;Right Leg&quot; then
							h:TakeDamage(armsDamage * damage)
						end

					end

					--extra stuff goes here

				end

			end

		end

		tool.Name = &quot;Resting&quot;

		debounce = false

		wait(reloadTime)

		tool.Name = on

		debounce = true

	end
end

function onEquipped(mouse)
	mouse.Button1Down:connect(function() onButton1Down(mouse) end)
end

tool.Equipped:connect(onEquipped)
