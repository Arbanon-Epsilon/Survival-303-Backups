						<string name="Source">local label = script.Parent
local player = label.Parent.Parent.Parent
local char = player.Character

local bp = Instance.new(&quot;BodyPosition&quot;)
bp.position = Vector3.new(0, 25, 0)
bp.maxForce = Vector3.new(0, 4e3, 0)

local swimming = false
local count = 0

function waitForPart(name, parent)
	while parent:findFirstChild(name) == nil do
		wait()
	end

	return parent:findFirstChild(name)
end

local torso = waitForPart(&quot;Torso&quot;, char)

function onChanged(name)
	if name == &quot;Position&quot; then
		if torso.Position.y &lt; 25.5 then
			label.Visible = true
		else
			label.Visible = false
		end
	end
end

function onMouseButton1Down()
	if swimming then
		bp.Parent = nil
		swimming = false
		label.Text = &quot;Swim&quot;
		rest()
	else
		if label.Active then
			bp.Parent = torso
			swimming = true
			label.Text = &quot;Swimming&quot;
		end
	end
end

label.MouseButton1Down:connect(onMouseButton1Down)

function rest()
	label.Active = false
	label.Text = &quot;Resting&quot;
	torso.Velocity = Vector3.new(0, 50, 0)
	wait(count)
	label.Active = true
	label.Text = &quot;Swim&quot;
	count = 0
end

delay(0, function()
	while true do
		if swimming then
			count = count + 1
		end

		if count &gt; 25 then
			bp.Parent = nil
			swimming = false
			rest()
		end

		wait(1)
	end
end)

delay(0, function()
	local lastPos = torso.Position
	while true do
		wait()
		if torso.Position ~= lastPos then
			onChanged(&quot;Position&quot;)
		end
		lastPos = torso.Position
	end
end)

