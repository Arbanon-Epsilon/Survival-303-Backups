					<string name="Source">local tool = script.Parent
local indexer = tool.Indexer
local frame = tool.Frame
local label = frame.Label

local current = nil
local example = nil

function getDist(a, b)
	return (a - b).magnitude
end

function indexerChanged(newVal)
	local player = tool.Parent.Parent
	local pack = player.Pack:GetChildren()
	local max = #pack

	if newVal &gt; max then
		indexer.Value = 1
	elseif newVal &lt; 1 then
		indexer.Value = max
	end

	current = pack[indexer.Value]

	if current ~= nil then
		label.Text = current.Name
		example = current:clone()
	else
		label.Text = &quot;&quot;
	end
end

indexer.Changed:connect(indexerChanged)

function onMouseMove(mouse)
	local player = tool.Parent.Parent
	local char = player.Character
	local head = char.Head

	if example ~= nil then
		example.Parent = char
		example.Anchored = true
		example.Position = mouse.hit.p

		if getDist(example.Position, head.Position) &gt; 15 then
			example.Transparency = 1
		else
			example.Transparency = 0.5
		end
	end
end

function onSelected(mouse)
	local player = tool.Parent.Parent

	frame.Parent = player.PlayerGui.ScreenGui

	mouse.Move:connect(function() onMouseMove(mouse) end)

	mouse.Button1Down:connect(function()
		if current ~= nil then
			local char = player.Character
			local head = char.Head

			if getDist(head.Position, mouse.hit.p) &lt; 15 then
				current.Parent = game.Workspace
				current.Position = mouse.hit.p
				current.Locked = false
				current = nil

				example:remove()
				example = nil

				indexer.Value = 0
			end
		end
	end)
end

function onDeselected()
	frame.Parent = tool

	if example ~= nil then
		example.Parent = nil
	end
end

