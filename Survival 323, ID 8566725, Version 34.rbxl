local frame = tool.Frame
local label = frame.Label

function getDist(a, b)
	return (a - b).magnitude
end

function getEdible(part)
	local val = part:findFirstChild(&quot;Edible&quot;)
	if val ~= nil then
		return true
	else
		return false
	end
end

function onMouseMove(mouse)
