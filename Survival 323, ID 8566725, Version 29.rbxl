		<Item class="Script" referent="RBX3">
			<Properties>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">StatGiver</string>
				<string name="Source">function modStat(stat, val)
	stat.Value = stat.Value + val

	if stat.Value &gt; 100 then
		stat.Value = 100
	end
end

function checkDead(statTable)
	for index, child in pairs(statTable) do
		if child.Value &lt; 1 then
			return true
		end
	end

	return false
end

function resetStats(statTable)
	for index, child in pairs(statTable) do
		child.Value = 100
	end
end

game.Players.ChildAdded:connect(function(player)
	local stats = Instance.new(&quot;IntValue&quot;)
	stats.Name = &quot;Stats&quot;

	local hunger = Instance.new(&quot;IntValue&quot;)
	hunger.Name = &quot;Hunger&quot;
	hunger.Value = 100
	hunger.Parent = stats

	local thirst = Instance.new(&quot;IntValue&quot;)
	thirst.Name = &quot;Thirst&quot;
	thirst.Value = 100
	thirst.Parent = stats

	local health = Instance.new(&quot;IntValue&quot;)
	health.Name = &quot;Health&quot;
	health.Value = 100
	health.Parent = stats

	stats.Parent = player

	local allStats = {hunger, thirst, health}

	while true do
		modStat(hunger, -1)
		modStat(thirst, -1)
		modStat(health, 1)

		if checkDead(allStats) then
			local char = player.Character
			local humanoid = char.Humanoid

			humanoid.Health = 0

			resetStats(allStats)
		end

		wait(15)
	end
end)</string>
				<bool name="archivable">true</bool>
