			<Item class="Script" referent="RBX36">
				<Properties>
					<bool name="Disabled">false</bool>
					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">Wander</string>
					<string name="Source">local torso = script.Parent.Torso
local human = script.Parent.Humanoid
local hp = human.Health
local height = torso.Position.y
local damage = 10

local weld = Instance.new(&quot;Weld&quot;)
weld.Parent = torso
weld.Part0 = torso
weld.Part1 = script.Parent.Head
weld.C1 = CFrame.new(Vector3.new(0, -torso.Size.y, torso.Size.z / 2))
weld.Name = &quot;Neck&quot;

wait(5)

function makeEdible()
	local c = script:GetChildren()

	for index, child in pairs(c) do
		child.Parent = torso
	end
end

function findNearestPlayer()
	local pos = torso.Position
	local best = 50
	local closest = nil
	local c = game.Players:GetChildren()

	for index, child in pairs(c) do
		local char = child.Character
		if char ~= nil then
			local t = char.Torso
			local dist = (t.Position - pos).magnitude
			if dist &lt; best then
				best = dist
				closest = char
			end
		end
	end

	if closest ~= nil then
		return closest
	end
end

function runFromPlayer(t)
	local unit = (t.Position - torso.Position).unit
	unit = -unit
	human:MoveTo(torso.Position + unit * human.WalkSpeed, game.Workspace.Base)
end

function attackPlayer(t)
	local unit = (t.Position - torso.Position).unit
	human:MoveTo(torso.Position + unit * human.WalkSpeed, game.Workspace.Base)

	local mag = (t.Position - torso.Position).magnitude
	if mag &lt; 10 and human.Health &gt; 0 then
		local h = t.Parent.Humanoid
		h:TakeDamage(damage)
	end
end

function takenDamage(chp)
	if chp &lt; hp then
		hp = chp
		return true
	end
	return false
end

function hasFallen(cheight)
	if cheight &lt; height then
		height = cheight
		return true
	end
	return false
end

function onDeath()
	torso.BrickColor = BrickColor.new(&quot;Bright red&quot;)
	torso.Name = &quot;Raw Meat&quot;
	torso.Locked = false
	torso.Parent = game.Workspace

	torso.BodyPosition:remove()

	makeEdible()

	for index, child in pairs(script.Parent:GetChildren()) do
		if child.className == &quot;Part&quot; then
			game:GetService(&quot;Debris&quot;):addItem(child, 5)
		end
	end
end

human.Died:connect(onDeath)

local count = 1

while true do

	if count == 5 then
		local rand = Vector3.new(math.random(-5, 5), math.random(-5, 5), math.random(-5, 5))  * 2

		human:MoveTo(torso.Position + rand, game.Workspace.Base)

		count = 0
	end

	if takenDamage(human.Health) == true then --Run!
		local char = findNearestPlayer()

		if char ~= nil then
			for run = 1, 25 do
				runFromPlayer(char.Torso)

				local canStop = findNearestPlayer()
				local fallen = hasFallen(torso.Position.y)

				if canStop == nil or canStop ~= char or fallen == true then --Is he gone? Am I about to run off a cliff?
					break
				end

				wait(1)
			end
		end
	end

	count = count + 1

	wait(1)
end
