		<Item class="Script" referent="RBX8">
			<Properties>
				<bool name="Disabled">true</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">OceanScript</string>
				<string name="Source">local ocean = Instance.new(&quot;Part&quot;)
ocean.Size = Vector3.new(500, 1, 500)
ocean.BrickColor = BrickColor.new(&quot;Deep blue&quot;)
ocean.Anchored = true
ocean.Locked = true

for x = -3, 3 do
for y = -3, 3 do
	local bottom = ocean:clone()
	bottom.Position = Vector3.new(x * 500, 0, y * 500)
	bottom.Parent = game.Workspace

	wait()

	local top = ocean:clone()
	top.Transparency = 0.5
	top.Position = Vector3.new(x * 500, 25, y * 500)
	top.Parent = game.Workspace

	wait()

	local edgeX = (x == -3 or x == 3)
	local edgeY = (y == -3 or y == 3)

	local wallX = ocean:clone()
	wallX.Size = Vector3.new(1, 25, 500)

	local wallY = ocean:clone()
	wallY.Size = Vector3.new(500, 25, 1)

	if edgeX then
		local pos = Vector3.new(x * 500 + (250 * x/3), 25/2, y * 500)

		local cX = wallX:clone()
		cX.Position = pos
		cX.Parent = game.Workspace
	end

	if edgeY then
		local pos = Vector3.new(x * 500, 25/2, y * 500 + (250 * y/3))

		local cY = wallY:clone()
		cY.Position = pos
		cY.Parent = game.Workspace
	end
end
end
</string>
